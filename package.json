{
  "name": "dekk",
  "version": "0.0.1",
  "description": "A presentation tool written in react.js",
  "repository": "git@github.com:sinnerschrader/dekk.git",
  "author": "Gregor Adams <greg@pixelass.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "babel": "lerna run babel",
    "babel:watch": "lerna run babel:watch --parallel",
    "build": "webpack",
    "clean": "lerna clean",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "develop": "npm-run-all --parallel babel:watch watch",
    "docs": "esdoc",
    "format:js": "lerna run format:js",
    "test": "lerna run test",
    "watch": "webpack-dev-server"
  },
  "workspaces": [
    "@dekk/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "esdoc": "^1.0.4",
    "esdoc-brand-plugin": "^1.0.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-importpath-plugin": "^1.0.1",
    "esdoc-lint-plugin": "1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "husky": "^0.14.3",
    "lerna": "^2.5.1",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.0.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  }
